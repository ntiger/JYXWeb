<md-toolbar class="md-warn" ng-cloak>
    <div class="md-toolbar-tools">
        <md-button class="md-icon-button" ng-click="$appUtil.openMenu()" hide-gt-sm aria-label="Toggle Menu">
            <md-icon class="material-icons">menu</md-icon>
        </md-button>
        <md-menu>
            <md-button aria-label="" ng-mouseenter="$mdMenu.open()" href="/Package/Index">
                我要转运
                <md-icon class="material-icons">keyboard_arrow_down</md-icon>
            </md-button>
            <md-menu-content width="4" ng-mouseleave="$mdMenu.close()" style="margin-top:64px">
                <md-menu-item>
                    <md-button href="/Package/New">
                        原装闪运
                    </md-button>
                </md-menu-item>
                <md-menu-item>
                    <md-button href="/Package/New?holdPackage=true">
                        普通转运
                    </md-button>
                </md-menu-item>
                <md-menu-item>
                    <md-button href="/Package/Index">
                        包裹管理
                    </md-button>
                </md-menu-item>
        </md-menu>
        <md-button aria-label="" href="/Home/Service">
            渠道说明
        </md-button>
        <md-button aria-label="" href="/Home/About">
            公司简介
        </md-button>
        <span flex></span>
        @Html.Partial("_LoginPartialToolBar")
    </div>
</md-toolbar>


