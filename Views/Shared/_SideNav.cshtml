<div>
    <md-sidenav class="md-sidenav-left md-whiteframe-z1 fill-height" md-component-id="left" md-is-locked-open="$mdMedia('gt-sm')"  ng-cloak>
        <md-toolbar md-scroll-shrink class="md-warn">
        <div class="md-toolbar-tools">
            <a href="/">
                <h1 class="docs-logotype md-heading ng-isolate-scope">美天优递</h1>
            </a>
         </div>
        </md-toolbar>
        <md-content flex role="navigation">
            <ul class="side-menu">
                <li data-ng-repeat="section in vm.$menu.sections" class="parent-list-item"
                    ng-class="{'parentActive' : vm.$menu.isSectionSelected(section)}">
                    <menu-link section="section" ng-if="section.type==='link'"></menu-link>
                    <menu-toggle section="section" ng-if="section.type==='toggle'"></menu-toggle>
                </li>
            </ul>
        </md-content>
    </md-sidenav>
</div>